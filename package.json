      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.11",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^1.9.6"
  }
